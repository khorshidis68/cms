@model CmsProject.DomainModelClass.PageGroup.PageGroup

@{
    ViewData["Title"] = "ایجاد گروه خبری";
}

<h2>ایجاد</h2>

<h4>گروه خبری</h4>
<hr />
<div class="row">
    <div class="col-md-12">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="GroupTitle" class="control-label"></label>
                <input asp-for="GroupTitle" class="form-control" />
                <span asp-validation-for="GroupTitle" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="ثبت" class="btn btn-success" />
                <a asp-action="Index" class="btn btn-info">بازگشت به لیست گروه خبر</a>
            </div>
        </form>
    </div>
</div>
